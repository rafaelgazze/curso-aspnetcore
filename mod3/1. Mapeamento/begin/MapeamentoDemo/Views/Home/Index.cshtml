@model MapeamentoDemo.Models.AlunosCursosViewModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    <div class="col-md-6">
        <h2>Alunos</h2>
        <ul>
            @foreach (var aluno in Model.Alunos)
            {
                <li>@aluno.Nome</li>
            }
        </ul>
    </div>
    <div class="col-md-6">
        <h2>Cursos</h2>
        <ul>
            @foreach (var curso in Model.Cursos)
            {
                <li>@curso.Nome</li>
            }
        </ul>
    </div>
</div>
